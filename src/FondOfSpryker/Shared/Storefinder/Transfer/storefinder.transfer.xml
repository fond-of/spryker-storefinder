<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="StorefinderCustomerAddress">
        <property name="storeId" type="int" />
        <property name="longitude" type="string" />
        <property name="latitude" type="string" />
        <property name="urlKey" type="string" />
        <property name="name" type="string" />
        <property name="city" type="string" />
        <property name="street" type="string" />
        <property name="zipCode" type="string" />
        <property name="phone" type="string" />
        <property name="mail" type="string" />
        <property name="website" type="string" />
        <property name="countryCode" type="string" />
    </transfer>

    <transfer name="StorefinderSearchRequest">
        <property name="storeId" type="int" />
        <property name="address" type="string" />
        <property name="countryCode" type="string" />
        <property name="limit" type="int" />
        <property name="limitStart" type="int" />
    </transfer>

    <transfer name="StorefinderGeometryRequest">
        <property name="address" type="string" />
        <property name="countryCode" type="string" />
    </transfer>

    <transfer name="StorefinderGeometryResponse">
        <property name="latitude" type="string" />
        <property name="longitude" type="string" />
    </transfer>

    <transfer name="StorefinderSearchResponse">
        <property name="resultCount" type="int" />
        <property name="result" type="StorefinderCustomerAddress[]" />
    </transfer>
</transfers>
